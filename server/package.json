  "bugs": {
    "url": "https://github.com/mchaconcr/distributed-blender-render/issues"
  },
  "homepage": "https://github.com/mchaconcr/distributed-blender-render#readme",
  "devDependencies": {
    "electron": "^9.3.0",
    "electron-packager": "^14.2.1",
    "electron-reload": "^1.5.0"
  },
  "dependencies": {
    "buffer-chunks": "^0.1.1",
    "electron-json-config": "^1.5.3",
    "fs-extra": "^9.0.0",
    "http": "0.0.1-security",
